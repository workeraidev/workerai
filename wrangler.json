{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "workerai",
  "main": "./src/worker/index.ts",
  "compatibility_date": "2025-04-01",
  "upload_source_maps": true,
  "compatibility_flags": ["nodejs_compat"],

  "observability": {
    "enabled": true
  },

  "ai": {
    "binding": "AI"
  },

  "browser": {
    "binding": "MYBROWSER"
  },

  "analytics_engine_datasets": [
    {
      "binding": "ANALYTICS",
      "dataset": "ANALYTICS"
    }
  ],

  "kv_namespaces": [
    {
      "binding": "CHAT_SESSIONS",
      "id": "3949af7168fd4640b667da4aee56c1b6",
      "preview_id": "3949af7168fd4640b667da4aee56c1b6"
    }
  ],

  "r2_buckets": [
    {
      "binding": "FILE_STORAGE",
      "bucket_name": "studioai",
      "preview_bucket_name": "studioai"
    }
  ],

  "durable_objects": {
    "bindings": [
      {
        "name": "CHAT_DO",
        "class_name": "ChatDurableObject"
      }
    ]
  },

  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": [
        "ChatDurableObject"
      ]
    }
  ],

  "d1_databases": [
    {
      "binding": "AI_CHAT_DB",
      "database_name": "studioai",
      "database_id": "b8f7a79f-71a1-412d-a79e-00f3a7a40dac"
    }
  ],

  "upload_source_maps": true,

  "assets": {
    "directory": "./dist/client",
    "not_found_handling": "single-page-application"
  }
}
