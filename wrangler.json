{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "workerai",
	"main": "./src/worker/index.ts",
	"compatibility_date": "2025-04-01",
	"compatibility_flags": ["nodejs_compat"],
	"observability": {
		"enabled": true
	},
	"ai": {
		"binding": "AI"
	},
	"kv_namespaces": [
		{
			"binding": "MY_KV",
			"id": "3949af7168fd4640b667da4aee56c1b6",
			"preview_id": "3949af7168fd4640b667da4aee56c1b6"
		}
	],
	"r2_buckets": [
		{
			"binding": "MY_BUCKET",
			"bucket_name": "studioai",
			"preview_bucket_name": "studioai"
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"name": "MY_DURABLE_OBJECT",
				"class_name": "YourDurableObjectClass"
			}
		]
	},
	"migrations": [
		{
			"tag": "v1",
			"new_sqlite_classes": ["YourDurableObjectClass"]
		}
	],
	"upload_source_maps": true,
	"assets": {
		"directory": "./dist/client",
		"not_found_handling": "single-page-application"
	}
}
